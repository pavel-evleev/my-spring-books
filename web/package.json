{
  "name": "MyReadedBooks",
  "version": "1.0.0",
  "description": "The app stores books that have been already read by user",
  "repository": {
    "type": "git",
    "url": "https://github.com/PavelEvleev/MyReadedBooks"
  },
  "author": {
    "name": "Pavel Evleev",
    "email": "mr.malefiic@gmail.com"
  },
  "contributors": [
    {
      "name": "Alex Soroka",
      "email": "lehaSVV2009@gmail.com"
    }
  ],
  "scripts": {
    "build": "webpack --config webpack.production.config.js --progress --profile --colors",
    "start": "webpack-dev-server --progress --profile --colors",
    "lint": "eslint --ext js --ext jsx src || exit 0",
    "dev": " webpack-dashboard -- webpack-dev-server",
    "test": "export NODE_ENV=test && jest"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "node-sass": "^4.5.3",
    "react": "15.6.1",
    "react-dom": "15.6.1",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "sass-loader": "^6.0.6"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "css-loader": "0.28.5",
    "enzyme": "^2.9.1",
    "eslint": "^4.5.0",
    "eslint-plugin-jest": "^20.0.3",
    "eslint-plugin-react": "^7.3.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.30.1",
    "jest": "^20.0.4",
    "loglevel": "^1.4.1",
    "postcss-loader": "^2.0.6",
    "react-addons-test-utils": "^15.6.0",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-test-renderer": "^15.6.1",
    "style-loader": "0.18.2",
    "url-loader": "0.5.9",
    "webpack": "^3.5.5",
    "webpack-cleanup-plugin": "^0.5.1",
    "webpack-dashboard": "^1.0.0-5",
    "webpack-dev-server": "^2.7.1"
  },
  "jest": {
    "verbose": true,
    "bail": true,
    "collectCoverage": true,
    "coverageDirectory": "coverage",
    "moduleNameMapper": {
      "\\.(jpe?g|png|gif|eot|otf|webp|svg|ttf|woff2?|mp[34]|webm|wav|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less|s[ac]ss|styl)$": "<rootDir>/__mocks__/styleMock.js",
      "app": "<rootDir>/src/app.jsx"
    },
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules"
    ]
  }
}
